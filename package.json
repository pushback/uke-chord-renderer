{
  "name": "uke-chord-renderer",
  "version": "0.1.0",
  "description": "Ukulele chord renderer.",
  "main": "index.js",
  "dependencies": {
    "@svgdotjs/svg.js": "^3.1.1",
    "svgdom": "^0.1.10",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "rimraf": "^3.0.2"
  },
  "scripts": {
    "start": "npm run build && node ./lib/index.js --all",
    "start:vertical": "npm run build && node ./lib/index.js --all --vertical",
    "sample": "npm run build && node ./lib/index.js Cm > ./dist/Cm.svg",
    "build": "npm run clean && mkdir -p ./dist && tsc -p .",
    "clean": "rimraf ./lib ./dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/**/*.ts"
  },
  "repository": "git+https://github.com/pushback/uke-chord-renderer.git",
